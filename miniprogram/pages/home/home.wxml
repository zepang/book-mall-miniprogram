<!--miniprogram/pages/home/home.wxml-->
<!-- <text>miniprogram/pages/home/home.wxml</text> -->
<import src="./templates/feed-card/feed-card.wxml"></import>
<block wx:if="{{tabIndex === 0}}">
  <view class="page">
    <include src="./templates/header.wxml"></include>
    <view class="feed-list">
      <view wx:for="{{list}}" wx:key="{{index}}" class="card">
        <template is="feed-card" data="{{item}}"></template>
      </view>
    </view>
  </view>
</block>
<block wx:elif="{{tabIndex === 1}}">
  <text>1</text>
</block>
<block wx:else="{{tabIndex === 2}}">
  <text>2</text>
</block>

<tab-bar bind:switch-tab="switchTab" selected="{{tabIndex}}"></tab-bar>

